<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <targets>
    <target xsi:type="File" layout="${longdate}|${level}|${processid}|${callsite:className=true:fileName=false:includeSourcePath=false:methodName=true}|${message}${onexception:inner=${newline}    |${exception:innerFormat=Message,Type,Method,StackTrace:maxInnerExceptionLevel=3:format=ToString}}" name="APILog" fileName="${event-context:InstanceSavePath}\logs\APILog-${shortdate}.log"/>
    <target xsi:type="File" layout="${longdate}|${level}|${processid}|${callsite:className=true:fileName=false:includeSourcePath=false:methodName=true}|${message}${onexception:inner=${newline}    |${exception:innerFormat=Message,Type,Method,StackTrace:maxInnerExceptionLevel=3:format=ToString}}" name="ServerLog" fileName="${event-context:InstanceSavePath}\logs\ServerLog-${shortdate}.log"/>
    <target xsi:type="File" layout="${longdate}|${level}|${processid}|${callsite:className=true:fileName=false:includeSourcePath=false:methodName=true}|${message}${onexception:inner=${newline}    |${exception:innerFormat=Message,Type,Method,StackTrace:maxInnerExceptionLevel=3:format=ToString}}" name="ChatLog" fileName="${event-context:InstanceSavePath}\logs\ChatLog-${shortdate}.log"/>
    <target xsi:type="File" layout="${longdate}|${level}|${processid}|${callsite:className=true:fileName=false:includeSourcePath=false:methodName=true}|${message}${onexception:inner=${newline}    |${exception:innerFormat=Message,Type,Method,StackTrace:maxInnerExceptionLevel=3:format=ToString}}" name="BaseLog" fileName="${basedir}\logs\SEServerExtenderLog-${shortdate}.log"/>
    <target xsi:type="ColoredConsole" layout="${longdate}|${level}|${processid}|${callsite:className=true:fileName=false:includeSourcePath=false:methodName=true}|${message}${onexception:inner=${newline}    |${exception:innerFormat=Message,Type,Method,StackTrace:maxInnerExceptionLevel=3:format=ToString}}" name="ConLog" />
  </targets>

  <rules>
    <logger name="ConLog" minlevel="Trace" writeTo="ConLog" />
    <logger name="APILog" minlevel="Trace" writeTo="APILog,ConLog" />
    <logger name="ServerLog" minlevel="Trace" writeTo="ServerLog,ConLog" />
    <logger name="ChatLog" minlevel="Trace" writeTo="ChatLog,ConLog" />
    <logger name="BaseLog" minlevel="Trace" writeTo="BaseLog,ConLog" />
  </rules>
</nlog>